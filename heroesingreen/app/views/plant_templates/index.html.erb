<h1>Listing plant_templates</h1>

    # name - string
    # cost - int
    # virility - int
    # image_path - string
    # layer_max - int
    # health_max - int
    #
    # water_min - int
    # water_max - int
    # water_consumption - int
    #
    # light_min - int
    # light_max - int
    #
    # nutrients_min - int
    # nutrients_max - int
    # nutrients_consumption - int
    #
    # vertical_rate - float
    # radial_rate - float
    # density_rate - float

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Image_Path</th>
	<th>Thumbnail</th>
    <th>Cost</th>
    <th>Virility</th>
    <th>Layer_Max</th>
    <th>Health_Max</th>
    <th>Wetness Tolerance</th>
    <th>Wetness Optimum</th>
    <th>Water consumption</th>
    <th>Light Min</th>
    <th>Light Max</th>
    <th>Nutrients Tolerance</th>
    <th>Nutrients Optimum</th>
    <th>Nutrients Consumption</th>
    <th>Vertical rate</th>
    <th>Radial rate</th>
    <th>Density rate</th>
  </tr>

<% @plant_templates.each do |plant_template| %>
  <tr>
    <td><%=h plant_template.name %></td>
    <td><%=h plant_template.description %></td>	
	<td><%=h plant_template.image_path %></td>
	<td><%=h plant_template.thumbnail %></td>
    <td><%=h plant_template.cost %></td>
    <td><%=h plant_template.virility %></td>
    <td><%=h plant_template.layer_max %></td>
    <td><%=h plant_template.health_max %></td>
    <td><%=h plant_template.wetness_tolerance %></td>
    <td><%=h plant_template.wetness_optimum %></td>
    <td><%=h plant_template.water_consumption %></td>
    <td><%=h plant_template.light_min %></td>
    <td><%=h plant_template.light_max %></td>
    <td><%=h plant_template.nutrients_tolerance %></td>
    <td><%=h plant_template.nutrients_optimum %></td>
    <td><%=h plant_template.nutrients_consumption %></td>
    <td><%=h plant_template.vertical_rate %></td>
    <td><%=h plant_template.radial_rate %></td>
    <td><%=h plant_template.density_rate %></td>
    <td><%= link_to 'Show', plant_template %></td>
    <td><%= link_to 'Edit', edit_plant_template_path(plant_template) %></td>
    <td><%= link_to 'Destroy', plant_template, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New plant_template', new_plant_template_path %>